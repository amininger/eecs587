sp {propose*initialize-graph-search
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-graph-search)
}

sp {apply*initialize-graph-search
   (state <s> ^operator <op>)
   (<op> ^name initialize-graph-search)
-->
   (<s> ^name graph-search
        ^path-cost 0.0
        ^parameters <p>)
   (<p> ^waypoint-storage smem
        ^mission-storage smem)}


