sp {elaborations*elaborate*state*digit-cue
   (state <s> ^digit <d>)
   (<d> ^word <word>
        ^number <number>)
-->
   (<d> ^cue <c>)
   (<c> ^word <word>
        ^number <number>)
}

sp {elaborations*elaborate*state*bad-retrieval*gm
   (state <s> ^epmem.result.graph-match 0)
-->
   (write |BAD RETRIEVAL!!!|)
   (halt)
}
